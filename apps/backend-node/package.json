{
    "name": "workorderpro-api-v3",
    "version": "1.0.0",
    "description": "Cloud-Native CMMS Backend (Node.js/Express)",
    "engines": {
        "node": ">=18.0.0"
    },
    "main": "dist/server.js",
    "scripts": {
        "build": "npx prisma generate && tsc",
        "start": "npx prisma db push --accept-data-loss && npx prisma db seed && node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.958.0",
        "@aws-sdk/s3-request-presigner": "^3.958.0",
        "@prisma/client": "5.10.2",
        "@types/bcrypt": "^6.0.0",
        "@types/express-rate-limit": "^5.1.3",
        "@types/uuid": "^10.0.0",
        "argon2": "^0.44.0",
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.3",
        "connect-pg-simple": "^10.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "express-rate-limit": "^8.2.1",
        "express-session": "^1.18.0",
        "helmet": "^7.1.0",
        "pg": "^8.16.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "uuid": "^13.0.0",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/connect-pg-simple": "^7.0.3",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.25",
        "@types/express-session": "^1.18.2",
        "@types/node": "^20.19.27",
        "@types/pg": "^8.16.0",
        "prisma": "5.10.2",
        "ts-node-dev": "^2.0.0"
    },
    "prisma": {
        "seed": "npx ts-node scripts/seed_roles_v2.ts"
    }
}
